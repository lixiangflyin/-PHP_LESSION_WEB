<?php
if ($_FILES['file']['error'] > 0)
{
	echo 'Error: ' . $_FILES['file']['error'];
}
else 
{
	echo 'Upload: ' . $_FILES['file']['name'] . "<br/>";
	echo 'Type: ' .   $_FILES['file']['type'] . "<br/>";
	echo 'Size: ' .   $_FILES['file']['size'] . "Bytes<br/>";
	echo 'Stored in: ' . $_FILES['file']['tmp_name'] . "<br/>";
}
?>